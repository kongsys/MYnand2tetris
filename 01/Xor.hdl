/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Not(in=a, out=w0);
    Not(in=b, out=w1);
    And(a=a, b=w1, out=w2);
    And(a=b, b=w0, out=w3);
    Or(a=w2, b=w3, out=out);
}